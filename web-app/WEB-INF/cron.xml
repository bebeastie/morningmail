<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/globalFeed</url>
    <description>Download global feeds every 15 minutes</description>
    <schedule>every 15 minutes synchronized</schedule>
  </cron>
  <cron>
    <url>/batchEmail</url>
    <description>Render, stage and send emails every 2 minutes</description>
    <schedule>every 2 minutes</schedule>
  </cron>
  <cron>
    <url>/batchEmail/keepAlive</url>
    <description>Keep alive</description>
    <schedule>every 1 minutes</schedule>
  </cron>
</cronentries>